{
    "name": "protrafficgroup/orchid-laraberg",
    "type": "library",
    "license": "MIT",
    "require": {
        "van-ons/laraberg": "^2.0",
        "orchid/platform": "^14.15"
    },
    "autoload": {
        "psr-4": {
            "ProTrafficGroup\\OrchidLaraberg\\": "src/"
        }
    },
    "scripts": {
        "post-package-install": [
            "php artisan vendor:publish --provider=\"VanOns\\Laraberg\\LarabergServiceProvider\""
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProTrafficGroup\\OrchidLaraberg\\LarabergServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Newind"
        }
    ]
}
